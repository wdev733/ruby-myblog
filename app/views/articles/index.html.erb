<h1>Home page</h1>

<p> <%= link_to 'Create an Artilce', new_article_path  %></p>
<table border="1">
    <tr>
    <th> Titles </th>
    <th>Descriptions</th>
  </tr>
  <% @articles.each do |ar| %>
  <tr>
    <td> <%= ar.title %> </td>
    <td> <%= ar.description %>  </td>
    <td> <%= link_to 'Edit', edit_article_path(ar) %> </td>
    <td> <%= link_to 'Show', article_path(ar) %> </td>
  </tr>

  <% end %>
  
</table>